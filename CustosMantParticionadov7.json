        - uuid: 143337baf37d46c6b8ac2ab9d0139706
          name: 'All Partition Sizes in MB'
          type: ODBC
          key: 'db.odbc.get[particionSizesMB,{$CTRLPART.DB.DSN},{$CTRLPART.DB.CONSTR}]'
          delay: 4h
          value_type: TEXT
          trends: '0'
          params: |
            SELECT 
                p.partition_ordinal_position AS partition_number,
                p.table_name,
                p.partition_method,
                SUM(p.data_length + p.index_length) AS partition_size,
                CONCAT(SUBSTRING(p.partition_name, 8, 2), "/",
                SUBSTRING(p.partition_name, 6, 2), "/", 
                SUBSTRING(p.partition_name, 2, 4) ) AS fecha
            FROM 
                information_schema.PARTITIONS p
            JOIN 
                information_schema.tables t
                ON p.table_schema = t.table_schema
                AND p.table_name = t.table_name
            WHERE 
                p.table_schema = 'zabbix' 
                AND p.partition_ordinal_position is not null
            GROUP BY 
                p.partition_ordinal_position, p.table_name, p.partition_method, p.partition_name
            ORDER BY 
                p.partition_ordinal_position;
          username: zabbix
          password: password
          tags:
            - tag: Application
              value: Control
            - tag: Application
              value: Particionado
            - tag: raw
